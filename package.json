{
  "name": "battles",
  "version": "0.0.1",
  "description": "Roguelike game situated in cold north",
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babelify": "^7.3.0",
    "blanket": "^1.2.3",
    "browserify": "^14.0.0",
    "file-saver": "^1.3.3",
    "gulp-cached": "^1.1.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-notify": "^3.0.0",
    "gulp-sass": "^3.1.0",
    "jquery": "^3.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "uglify-es": "^3.0.25",
    "vinyl-source-stream": "^1.1.0",
    "why-did-you-update": "0.0.8"
  },
  "devDependencies": {
    "babel-cli": "~6.10.1",
    "babel-eslint": "^7.1.1",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "browserify-incremental": "^3.1.1",
    "chai": "~3.5.0",
    "coveralls": "^2.12.0",
    "eslint": "^3.14.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prefer-object-spread": "^1.1.0",
    "eslint-plugin-react": "^6.9.0",
    "istanbul": "~0.4.5",
    "jsctags": "git://github.com/ramitos/jsctags.git",
    "mocha": "~3.1",
    "mocha-istanbul": "*",
    "node-localstorage": "~1.3.0",
    "sass-lint": "^1.10.2"
  },
  "contributors": [
    "Tuomas Poikela <tuomas.sakari.poikela@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/tpoikela/battles.git"
  },
  "scripts": {
    "build": "npm run build:sass && npm run build:production",
    "build:production": "NODE_ENV=production ./browserify ./client/jsx/app.jsx --extension .jsx -t babelify | uglifyjs --mangle --compress > ./build/bundle.js",
    "build:production:debug": "NODE_ENV=production ./browserify --debug ./client/jsx/app.jsx --extension .jsx -t babelify > ./build/bundle.js",
    "build:sass": "sass scss/style.scss -t compressed > build/style.css",
    "clean": "rm build/*",
    "coverage": "perl bin/cov_test.pl",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "hooks": "perl bin/deploy_hooks",
    "start": "python -m SimpleHTTPServer",
    "test": "mocha --recursive tests/client/src",
    "test:functional": "mocha --recursive tests/client/functional/",
    "test:gui": "mocha --recursive tests/client/gui",
    "test:all": "npm run test && npm run test:gui && npm run test:functional"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tpoikela/battles/issues"
  }
}
